{
  "presets": [
    "@babel/typescript",
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "> 1%",
            "Chrome >= 48",
            "Firefox >= 44",
            "Firefox ESR",
            "Safari >= 9.1",
            "Edge >= 12",
            "ios_saf >= 9.3",
            "ie 11"
          ]
        },
        "modules": false,
        "forceAllTransforms": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}